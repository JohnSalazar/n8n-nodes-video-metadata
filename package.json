{
  "name": "n8n-nodes-video-metadata",
  "version": "1.2.1",
  "description": "n8n node for extracting video metadata using ffprobe with embedded static binaries",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "video",
    "metadata",
    "ffprobe",
    "ffmpeg",
    "video-metadata",
    "video-info",
    "media"
  ],
  "license": "MIT",
  "homepage": "https://github.com/JohnSalazar/n8n-nodes-video-metadata",
  "author": {
    "name": "Joao Salazar",
    "email": "joaobosco.salazar@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JohnSalazar/n8n-nodes-video-metadata.git"
  },
  "bugs": {
    "url": "https://github.com/JohnSalazar/n8n-nodes-video-metadata/issues"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes/**/*.ts package.json",
    "lintfix": "eslint nodes/**/*.ts package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/VideoMetadata/VideoMetadata.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.40.0",
    "eslint-plugin-n8n-nodes-base": "^1.12.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
