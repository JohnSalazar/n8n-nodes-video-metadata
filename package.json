{
  "name": "n8n-nodes-video-metadata",
  "version": "1.0.0",
  "description": "n8n node for extracting video metadata using ffprobe with embedded static binaries",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "video",
    "metadata",
    "ffprobe",
    "ffmpeg"
  ],
  "license": "MIT",
  "homepage": "https://github.com/seu-usuario/n8n-nodes-video-metadata",
  "author": {
    "name": "Seu Nome",
    "email": "seu@email.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seu-usuario/n8n-nodes-video-metadata.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes package.json --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/VideoMetadata/VideoMetadata.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.40.0",
    "eslint-plugin-n8n-nodes-base": "^1.12.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}